<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <servlet>
        <servlet-name>GetHTMLContentServlet</servlet-name>
        <servlet-class>sample.Servlet.GetHTMLContentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetElementContentServlet</servlet-name>
        <servlet-class>sample.Servlet.GetElementContentServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ScrappingServlet</servlet-name>
        <servlet-class>sample.Servlet.ScrappingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetHTMLContentServlet</servlet-name>
        <url-pattern>/GetHTMLContentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetElementContentServlet</servlet-name>
        <url-pattern>/GetElementContentServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ScrappingServlet</servlet-name>
        <url-pattern>/ScrappingServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>welcome.html</welcome-file>
    </welcome-file-list>
</web-app>
